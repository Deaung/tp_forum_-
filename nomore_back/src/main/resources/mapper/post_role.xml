<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korit.nomoreback.domain.postRole.PostRoleMapper">


    <insert id="insertPostRole" useGeneratedKeys="true" keyProperty="postRoleId">
        insert into post_role_tb(post_role_name, user_id, post_id)
        values (#{postRoleName},#{userId},#{postId})
    </insert>

</mapper>
