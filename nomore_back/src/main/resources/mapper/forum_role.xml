<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korit.nomoreback.domain.forum.ForumRoleMapper">

    <insert id="insertForumRole" useGeneratedKeys="true" keyProperty="forumRoleId">
        insert into forum_role_tb(user_id, forum_id, forum_role_name)
        values (#{userId},#{forumId},#{forumRoleName})
    </insert>

</mapper>
